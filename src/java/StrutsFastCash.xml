 <struts>
   <constant name="struts.devMode" value="true" />
    <package name="com.syntelinc.BOK.ATM.fastcash" extends="struts-default">
        <action name="getAmount" 
              class="com.syntelinc.BOK.ATM.fastcash.FastCash" 
              method="execute">
            <result name="input">/Views/FastCash/FastCash.jsp</result>
            <result name="error">/Views/FastCash/</result>
            <result name="success">/Views/Withdrawals/WithdrawalSuccess.jsp</result>
        </action>
    </package>
</struts>
